<h1>Listing workflow_components</h1>

<table>
  <tr>
    <th>Workflow</th>
    <th>License</th>
    <th>Version</th>
    <th>Family</th>
    <th>Name</th>
    <th>Registry</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @workflow_components.each do |workflow_component| %>
  <tr>
    <td><%= workflow_component.workflow_id %></td>
    <td><%= workflow_component.license_id %></td>
    <td><%= workflow_component.version %></td>
    <td><%= workflow_component.family %></td>
    <td><%= workflow_component.name %></td>
    <td><%= workflow_component.registry %></td>
    <td><%= link_to 'Show', workflow_component %></td>
    <td><%= link_to 'Edit', edit_workflow_component_path(workflow_component) %></td>
    <td><%= link_to 'Destroy', workflow_component, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Workflow component', new_workflow_component_path %>
